    "@mui/x-date-pickers": "^5.0.19",
    "@reduxjs/toolkit": "^1.9.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "axios": "1.2.0",
    "clipboard-copy": "^4.0.1",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "env-cmd": "^10.1.0",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "lodash-joins": "^3.1.1",
    "moment": "^2.29.4",
    "plotly.js": "^2.23.2",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-plotly.js": "^2.6.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.3",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.2",
    "react-type-animation": "^3.1.0",
    "redux-logger": "^3.0.6",
    "sass": "^1.58.0",
    "socket.io-client": "^4.6.1",
    "tailwind-scrollbar": "^2.1.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "dev:tailwind": "tailwindcss -i src/styles/tailwind.css -o src/styles/index.css -w",
    "dev:react": "env-cmd -f ./.env react-scripts start",
    "start": "npm-run-all --parallel dev:*",
    "build": "env-cmd -f ./.env react-scripts build",
    "build:dev": "env-cmd -f ./.env.development react-scripts build",
    "build_test_env": "env-cmd -f ./.env.test react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --env=jsdom --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "typescript": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint src --color",
    "prettier": "prettier src --check",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint-staged": "lint-staged --config lint-staged.js",
    "husky-install": "cd .. && husky install web/.husky",
    "start-auth": "node server.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/plotly.js": "^2.12.18",
    "@types/react-plotly.js": "^2.6.0",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.0",
    "lint-staged": "^13.1.1",
    "multer": "^1.4.5-lts.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "sql-formatter": "^12.2.1",
    "tailwindcss": "^3.4.14",
    "tsc-files": "^1.1.3"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios)/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/stories/*",
      "!src/index.tsx",
      "!src/client.ts",
      "!src/urls.ts",
      "!src/authConfig.ts"
    ]
  }
}
